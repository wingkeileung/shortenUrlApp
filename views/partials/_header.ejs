<header>Wecome to Tiny URL App</header>
<% if (username) { %>

<form method="POST" action=/logout>
<button type="delete" >Logout</button></form>
<h2> <%= username %> </h2>
<% } else { %>

<form method="POST" action=/login>
<label for="username">Username here</label>
      <input id="username" type="text" name="username" placeholder="name=username" style="width: 120px">
      <input type="submit" value="Submit">
</form>
<% } %>
